count the odd numbers between low and high


/**
 * @param {number} low
 * @param {number} high
 * @return {number}
 */
var countOdds = function(low, high) {
    let sum = 0;
    for(let i = low ; i <= high ; i++){
        if(i%2 == 1){
            sum += 1;
        }
       
    }
     return sum;
};
